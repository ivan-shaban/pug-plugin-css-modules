{
  "name": "pug-plugin-css-modules",
  "version": "0.1.0",
  "description": "Plugin for for 'pug' template language to enable 'css modules'",
  "main": "lib/index.js",
  "author": "Ivan Shaban <ishaban.flash@gmail.com> (https://github.com/ivan-shaban)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ivan-shaban/pug-plugin-css-modules.git"
  },
  "keywords": [
    "Pug",
    "CSS Modules"
  ],
  "scripts": {
    "build": "babel ./src --out-dir ./lib"
  },
  "dependencies": {
    "extend-shallow": "^2.0.1",
    "postcss": "^5.2.12",
    "postcss-modules-sync": "^1.0.0",
    "pug-walk": "^1.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.18.0",
    "babel-preset-es2015": "^6.18.0"
  },
  "babel": {
    "presets": ["es2015"],
    "plugins": ["transform-class-properties"]
  }
}
